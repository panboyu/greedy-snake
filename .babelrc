{
    "presets": [
        [
            "@babel/env", 
            {
                "modules": false
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": {
                    "version": 3,
                    "proposals": true
                  },
                  "useESModules": true
            }
        ]
    ]
}

